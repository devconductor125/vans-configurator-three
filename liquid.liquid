  function isValueInScale(value, scale, radius, vanDiff) {
    for (let i = 0; i < scale.length; i += 2) {
        if (value + 100 >= (scale[i] + radius - vanDiff + 100 ) && value + 100 <= (scale[i + 1] - radius - vanDiff + 100)) {
          return i;
        }
    }
    return -1;
  }

